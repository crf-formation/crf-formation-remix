{
  "name": "indie-stack-template",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "// postinstall": "patch-package --patch-dir other/patches",
    "// preinstall": "npx only-allow pnpm",
    "build": "run-s build:*",
    "build:remix": "remix build",
    "dev": "cross-env NODE_ENV=development remix dev",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "npm run build && prisma generate && prisma migrate deploy && prisma db seed && playwright install",
    "prisma:setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "prisma:migrate": "prisma migrate dev --name ",
    "prisma:studio": "prisma studio",
    "start": "remix-serve build",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:e2e": "npm run test:e2e:dev --silent",
    "test:e2e:dev": "playwright test --ui",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env CI=true playwright test",
    "test:e2e:install": "npx playwright install --with-deps chromium",
    "typecheck": "tsc",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "gf:validate": "gf",
    "test-typecheck": "tsc -b -v"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/playwright-report"
  ],
  "dependencies": {
    "@emotion/cache": "^11.10.3",
