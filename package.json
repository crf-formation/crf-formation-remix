{
  "name": "indie-stack-template",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix build",
    "dev": "run-p dev:* | pino-pretty",
    "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev | pino-pretty",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "prisma:setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "remix-serve build",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "yarn run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "gf:validate": "gf",
    "test-typecheck": "tsc -b -v",
    "prisma:migrate": "prisma migrate dev --name "
  },
  "pre-commit": [
    "lint",
    "validate-good-fence",
    "test"
  ],
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@emotion/cache": "^11.10.3",
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.4",
    "@loadable/component": "^5.15.2",
    "@mui/icons-material": "^5.10.3",
    "@mui/lab": "^5.0.0-alpha.98",
    "@mui/material": "^5.10.4",
    "@prisma/client": "^4.2.1",
    "@remix-run/express": "^1.14.0",
    "@remix-run/node": "^1.14.0",
    "@remix-run/react": "^1.14.0",
    "@remix-run/router": "^1.3.3",
    "@remix-run/serve": "^1.14.0",
    "@remix-run/server-runtime": "^1.14.0",
    "@remix-run/web-blob": "^3.0.4",
    "@remix-run/web-fetch": "^4.3.2",
    "@remix-run/web-file": "^3.0.2",
    "@remix-run/web-form-data": "^3.0.4",
    "@remix-run/web-stream": "^1.0.3",
    "@remix-validated-form/with-zod": "^2.0.5",
    "@types/lodash": "^4.14.188",
    "bcryptjs": "^2.4.3",
    "date-fns": "^2.29.3",
    "express": "^4.17.1",
    "hibp": "^11.1.0",
    "history": "^5.1.0",
    "intl-parse-accept-language": "^1.0.0",
    "ip-regex": "^5.0.0",
    "is-ip": "^5.0.0",
    "isbot": "^3.5.1",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "pino": "^8.5.0",
    "pino-pretty": "^9.1.0",
    "prop-types": "^15.8.1",
    "ra-core": "^4.8.1",
    "ra-data-json-server": "^4.3.1",
    "react": "^18.2.0",
    "react-admin": "4.4.3",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.40.0",
    "react-json-view": "^1.21.3",
    "react-router": "^6.1.0",
    "react-router-dom": "^6.1.0",
    "remix-validated-form": "^4.6.5",
    "tiny-invariant": "^1.2.0",
    "ua-parser-js": "^1.0.2",
    "uuid": "^9.0.0",
    "zod": "^3.20.2",
    "zod-form-data": "^1.2.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.4.0",
    "@remix-run/dev": "^1.14.0",
    "@remix-run/eslint-config": "^1.14.0",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.6",
    "@types/node": "^18.7.13",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-react": "^2.0.1",
    "@vitest/coverage-c8": "^0.22.1",
    "autoprefixer": "^10.4.8",
    "binode": "^1.0.5",
    "c8": "^7.12.0",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "cypress": "^10.6.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "good-fences": "^1.1.2",
    "happy-dom": "^6.0.4",
    "msw": "^0.49.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "prettier": "2.7.1",
    "prisma": "^4.2.1",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.9",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.22.1"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
